    "knex": "^0.21.5",
    "pg": "^8.3.2",
    "validator": "^13.1.1",
    "ytdl-core": "^3.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "jest": "^26.4.2",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0",
    "ts-standard": "^9.0.0",
    "typescript": "^4.0.5"
  }
